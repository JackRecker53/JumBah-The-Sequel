{
  "name": "jumbah-travel-app",
  "version": "2.0.0",
  "description": "JumBah - Your Adventure Companion in Sabah. A comprehensive travel application featuring AI-powered trip planning, interactive maps, cultural exploration, and gamified experiences.",
  "private": true,
  "scripts": {
    "setup": "node setup-check.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd Backend && python -m uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "dev:frontend": "cd Frontend && npm run dev",
    "install:all": "cd Backend && pip install -r requirements.txt && cd ../Frontend && npm install",
    "build": "cd Frontend && npm run build",
    "test": "cd Frontend && npm run lint"
  },
  "keywords": [
    "travel",
    "sabah",
    "tourism",
    "ai-planner",
    "maps",
    "cultural-exploration",
    "adventure",
    "malaysia"
  ],
  "author": "JumBah Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/JumBah-The-Sequel.git"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "python": ">=3.8.0"
  }
}